using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class if_else_statement : MonoBehaviour
{
    public int age;
    // Start is called before the first frame update
    void Start()
    {
        if (age > 18)
        {
            Debug.Log("person is valid to vote");
        }
        else
        {
            Debug.Log("person is not valid to vote");
        }
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
